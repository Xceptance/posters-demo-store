<?xml version="1.0" encoding="UTF-8" ?>
<scriptmodule version="3" xmlns="http://xlt.xceptance.com/xlt-script/2">
  <description>Fills in the complete address form.</description>
  <parameter name="name"/>
  <parameter name="company"/>
  <parameter name="address"/>
  <parameter name="city"/>
  <parameter name="state"/>
  <parameter name="zip"/>
  <command name="type" target="id=fullName" value="@{name}"/>
  <command name="type" target="id=company" value="@{company}"/>
  <command name="type" target="id=addressLine" value="@{address}"/>
  <command name="type" target="id=city" value="@{city}"/>
  <command name="type" target="id=state" value="@{state}"/>
  <command name="type" target="id=zip" value="@{zip}"/>
</scriptmodule>
