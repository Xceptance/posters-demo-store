<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="7">
  <description>Verifies the adresses of my account</description>
  <parameter name="adressId"/>
  <parameter name="fullName"/>
  <parameter name="company"/>
  <parameter name="addressLine"/>
  <parameter name="city"/>
  <parameter name="state"/>
  <parameter name="zip"/>
  <parameter name="country"/>
  <command name="assertText" target="css=#@{adressId}  .name" value="@{fullName}"/>
  <command name="assertText" target="css=#@{adressId}  .company" value="@{company}"/>
  <command name="assertText" target="css=#@{adressId}  .addressLine" value="@{addressLine}"/>
  <command name="assertText" target="css=#@{adressId}  .city" value="@{city}"/>
  <command name="assertText" target="css=#@{adressId}  .state" value="@{state}"/>
  <command name="assertText" target="css=#@{adressId}  .zip" value="@{zip}"/>
  <command name="assertText" target="css=#@{adressId}  .country" value="@{country}"/>
</scriptmodule>