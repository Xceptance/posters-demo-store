<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="6">
  <description>Fills in the complete registration form.</description>
  <parameter name="lastName"/>
  <parameter name="firstName"/>
  <parameter name="email"/>
  <parameter name="password"/>
  <parameter name="passwordAgain"/>
  <command name="type" target="id=lastName" value="@{lastName}"/>
  <command name="type" target="id=firstName" value="@{firstName}"/>
  <command name="type" target="id=eMail" value="@{email}"/>
  <command name="type" target="id=password" value="@{password}"/>
  <command name="type" target="id=passwordAgain" value="@{passwordAgain}"/>
</scriptmodule>