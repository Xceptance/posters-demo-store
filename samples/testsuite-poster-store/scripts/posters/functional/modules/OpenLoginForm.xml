<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="6">
  <description>Opens the log-in form.</description>
  <action name="OpenLoginForm"/>
  <command name="assertElementPresent" target="id=btnShowLoginForm"/>
  <command name="click" target="css=#showLoginForm"/>
  <command name="waitForElementPresent" target="id=userMenu"/>
  <command name="clickAndWait" target="css=#userMenu .goToLogin"/>
</scriptmodule>